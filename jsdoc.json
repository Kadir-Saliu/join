{
  "source": {
    "include": ["./script.js", "./scripts/"],
    "includePattern": "\\.(js)$",
    "exclude": ["node_modules/", "out/"]
  },
  "opts": {
    "destination": "./out/",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
